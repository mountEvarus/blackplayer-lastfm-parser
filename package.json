{
  "name": "@mountevarus/blackplayer-lastfm-parser",
  "version": "0.2.0",
  "author": "Evan Hynes <ewbhynes@gmail.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run build.js",
    "format": "prettier --write src --config ./.prettierrc",
    "format:check": "prettier --check src --config ./.prettierrc",
    "lint": "eslint src --ext .ts",
    "start": "bun --watch src/index.ts",
    "start:example": "bun --watch src/index.ts -p ./example/statistics.bpstat -i /storage/emulated/0/Music -o ./example/music",
    "ts-check": "tsc --noemit"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "music-metadata": "10.6.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/bun": "1.1.6",
    "@types/lodash": "4.17.13",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "bun-plugin-dts": "0.3.0",
    "bun-types": "1.1.24",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-no-relative-import-paths": "1.5.3",
    "knip": "5.10.0",
    "prettier": "3.1.0",
    "typescript": "4.7.4"
  }
}
